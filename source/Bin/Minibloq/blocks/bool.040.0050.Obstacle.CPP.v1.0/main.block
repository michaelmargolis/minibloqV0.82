<?xml version="1.0" encoding="UTF-8"?>
<block xmlns="http://block.namespaces.minibloq.org" xmlns:minibloq="http://minibloq.org" MinibloqVersion="0.8.Beta">
    <properties>
		<name>Obstacle</name>
		<function>Getter</function>
		<loadAction>load</loadAction>
    </properties>
    <code>
		<dataType>bool</dataType>
		<initialCode>
			<line0>lookForObstacle(</line0>
		</initialCode>
		<methodSeparator></methodSeparator>
		<defaultMethodName>)</defaultMethodName>
	</code>
    <interface>
		<originalSize>
			<width>mediumGettersDefaultWidth::</width>
			<height>blockDefaultHeight::</height>
		</originalSize>
        <childFactor>0.9</childFactor>
        <defaultBackgroundColour0>
			<R>getterBlockColour0R::</R>
			<G>getterBlockColour0G::</G>
			<B>getterBlockColour0B::</B>
		</defaultBackgroundColour0>
        <defaultBackgroundColour1>
			<R>getterBlockColour1R::</R>
			<G>getterBlockColour1G::</G>
			<B>getterBlockColour1B::</B>
		</defaultBackgroundColour1>
		<picker>
			<imagesBasePath>thisBlockPath::</imagesBasePath>
			<default>Images/PickerGetterDefault.png</default>
			<hover>Images/PickerHover.png</hover>
			<pressed>Images/PickerGetterPressed.png</pressed>
			<disabled>Images/PickerDisabled.png</disabled>
		</picker>
		<blockButton>
			<has>true</has>
			<imagesBasePath>thisBlockPath::</imagesBasePath>
			<default>Images/Block.png</default>
			<disabled>Images/BlockDisabled.png</disabled>
		</blockButton>
		<methodButton>
			<has>true</has>
			<imagesBasePath>thisBlockPath::</imagesBasePath>
			<default>Images/QuestionMark.png</default>
			<hover>Images/QuestionMark.png</hover>
			<pressed>Images/QuestionMark.png</pressed>
			<disabled>Images/QuestionMark.png</disabled>
		</methodButton>
		<hasInstanceNameField>true</hasInstanceNameField>
		<instanceNameFieldSorted>false</instanceNameFieldSorted>
		<instanceType>Obstacle</instanceType>
	</interface>
</block>
